<h1>Listing items</h1>

<table>
  <tr>
    <th>Extenstion</th>
    <th>Pages positions</th>
    <th>Language</th>
    <th>User</th>
  </tr>

<% for item in @items %>
  <tr>
    <td><%=h item.extenstion_id %></td>
    <td><%=h item.pages_positions_id %></td>
    <td><%=h item.language_id %></td>
    <td><%=h item.user_id %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New item', new_item_path %>
