<h1>Listing subevents</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Event date</th>
    <th>Content</th>
  </tr>

<% for subevent in @subevents %>
  <tr>
    <td><%=h subevent.event_id %></td>
    <td><%=h subevent.event_date %></td>
    <td><%=h subevent.content %></td>
    <td><%= link_to 'Show', subevent %></td>
    <td><%= link_to 'Edit', edit_subevent_path(subevent) %></td>
    <td><%= link_to 'Destroy', subevent, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New subevent', new_subevent_path %>
